{
  "twiage": {
    "logging": {
      "namespace": "twiage-logging",
      "meta": {},
      "headers": [],
      "exitOnError": true,
      "transports": [
        {
          "type": "Console",
          "opts": {
            "level": "debug",
            "handleExceptions": true,
            "json": true,
            "colorize": false,
            "timestamp": true,
            "stringify": true,
            "humanReadableUnhandledException": true
          }
        }
      ]
    }
  }
}
